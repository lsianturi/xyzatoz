<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" 
		"http://struts.apache.org/dtds/struts-config_1_2.dtd">

<struts-config>
	<form-beans>
		<form-bean name="ajuSearch" type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="dispatch" type="java.lang.String" initial="prepare" />
			<form-property name="noKredit" type="java.lang.String" />
			<form-property name="nik" type="java.lang.String" />
			<form-property name="tglAjuFrom" type="java.lang.String" />
			<form-property name="tglAjuTo" type="java.lang.String" />
			<form-property name="perusahaan" type="java.lang.Integer" />
			<form-property name="nasabah" type="java.lang.String" />
			<form-property name="tipeKredit" type="java.lang.Integer" />
			<form-property name="jenisPinjam" type="java.lang.Integer" />
			<form-property name="sponsor" type="java.lang.Integer" />
			<form-property name="marketing" type="java.lang.Integer"/>
			<form-property name="surveyor" type="java.lang.Integer"/>
			<form-property name="status" type="java.lang.Integer"/>
			<form-property name="pageIndex" type="java.lang.String" initial="1" />
			<form-property name="totalPage" type="java.lang.String" />
			<form-property name="totalSize" type="java.lang.String" />		
		</form-bean>
		
		<form-bean name="ajuForm" type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="dispatch" type="java.lang.String" />
			<form-property name="usercode" type="java.lang.String" />
			<form-property name="id" type="java.lang.Integer" />
			<form-property name="noKredit" type="java.lang.String" />
			<form-property name="tglAju" type="java.lang.String" />
			<form-property name="nsbhId" type="java.lang.Integer" />
			<form-property name="nsbhNama" type="java.lang.String" />
			<form-property name="nsbhAlamat" type="java.lang.String" />
			<form-property name="nsbhDomisili" type="java.lang.String" />
			<form-property name="nsbhJnsKelamin" type="java.lang.String" />
			<form-property name="nsbhNik" type="java.lang.String" />
			<form-property name="nsbhBagian" type="java.lang.String" />
			<form-property name="nsbhPt" type="java.lang.String" />
			<form-property name="nsbhBank" type="java.lang.String" />
			<form-property name="nsbhTelepon" type="java.lang.String" />
			<form-property name="nsbhNoRekening" type="java.lang.String" />
			<form-property name="nsbhTglPayrol" type="java.lang.String" />
			<form-property name="nsbhAplikasi" type="java.lang.String" />
			<form-property name="penjamin" type="java.lang.String" />
			<form-property name="agunan" type="java.lang.String" />
			<form-property name="sponsor" type="java.lang.Integer" />
			<form-property name="surveyor" type="java.lang.Integer"/>
			<form-property name="marketing" type="java.lang.Integer"/>
			<form-property name="tipeKredit" type="java.lang.Integer" initial="1"/>
			<form-property name="jenisPinjam" type="java.lang.Integer" />
			<form-property name="jumlahAju" type="java.lang.String" />
			<form-property name="tenor" type="java.lang.Integer" />
			<form-property name="interestRate" type="java.lang.String"/>
			<form-property name="bunga" type="java.lang.String"/>
			<form-property name="angsuranAju" type="java.lang.String" />
			<form-property name="jatuhTempo" type="java.lang.String" />
			<form-property name="note" type="java.lang.String" />
		</form-bean>
		
		<form-bean name="realSearch" type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="dispatch" type="java.lang.String" initial="prepare" />
			<form-property name="noKredit" type="java.lang.String" />
			<form-property name="nik" type="java.lang.String" />
			<form-property name="tglAjuFrom" type="java.lang.String" />
			<form-property name="tglAjuTo" type="java.lang.String" />
			<form-property name="perusahaan" type="java.lang.Integer" />
			<form-property name="nasabah" type="java.lang.String" />
			<form-property name="tipeKredit" type="java.lang.Integer" />
			<form-property name="jenisPinjam" type="java.lang.Integer" />
			<form-property name="sponsor" type="java.lang.Integer" />
			<form-property name="marketing" type="java.lang.Integer"/>
			<form-property name="status" type="java.lang.Integer"/>
			<form-property name="pageIndex" type="java.lang.String" initial="1" />
			<form-property name="totalPage" type="java.lang.String" />
			<form-property name="totalSize" type="java.lang.String" />		
		</form-bean>
		
		<form-bean name="realForm" type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="dispatch" type="java.lang.String" />
			<form-property name="usercode" type="java.lang.String" />
			<form-property name="id" type="java.lang.Integer" />
			<form-property name="noKredit" type="java.lang.String" />
			<form-property name="noUrut" type="java.lang.Integer" />
			<form-property name="tglAju" type="java.lang.String" />
			<form-property name="nsbhId" type="java.lang.Integer" />
			<form-property name="nsbhNama" type="java.lang.String" />
			<form-property name="nsbhAlamat" type="java.lang.String" />
			<form-property name="nsbhDomisili" type="java.lang.String" />
			<form-property name="nsbhJnsKelamin" type="java.lang.String" />
			<form-property name="nsbhNik" type="java.lang.String" />
			<form-property name="nsbhBagian" type="java.lang.String" />
			<form-property name="nsbhPt" type="java.lang.String" />
			<form-property name="nsbhBank" type="java.lang.String" />
			<form-property name="nsbhTelepon" type="java.lang.String" />
			<form-property name="nsbhNoRekening" type="java.lang.String" />
			<form-property name="nsbhTglPayrol" type="java.lang.String" />
			<form-property name="nsbhAplikasi" type="java.lang.String" />
			<form-property name="penjamin" type="java.lang.String" />
			<form-property name="agunan" type="java.lang.String" />
			<form-property name="sponsor" type="java.lang.Integer" />
			<form-property name="marketing" type="java.lang.Integer"/>
			<form-property name="surveyor" type="java.lang.Integer"/>
			<form-property name="tipeKredit" type="java.lang.Integer" />
			<form-property name="jenisPinjam" type="java.lang.Integer" />
			<form-property name="jumlahAju" type="java.lang.String" />
			<form-property name="tenor" type="java.lang.Integer" />
			<form-property name="interestRate" type="java.lang.String"/>
			<form-property name="bunga" type="java.lang.String"/>
			<form-property name="angsuranAju" type="java.lang.String" />
			<form-property name="jatuhTempo" type="java.lang.String" />
			<form-property name="tglReal" type="java.lang.String" />
			<form-property name="jumlahReal" type="java.lang.String" />
			<form-property name="biayaAdmin" type="java.lang.String" />
			<form-property name="biayaProvisi" type="java.lang.String" />
			<form-property name="biayaLain" type="java.lang.String" />
			<form-property name="sisaAngsuran" type="java.lang.String" />
			<form-property name="angsuranReal" type="java.lang.String" />
			<form-property name="diterima" type="java.lang.String" />
			<form-property name="terbilang" type="java.lang.String" />
			<form-property name="keterangan" type="java.lang.String" />
			<form-property name="lunas" type="java.lang.String" />
			
		</form-bean>
		
		
	</form-beans>
	
	<action-mappings>
		<action path="/trx/aju" name="ajuSearch" type="com.benclaus.koperasi.action.trx.AjuAction" scope="request" validate="false" parameter="dispatch">
    		<forward name="continue" path="/app/trx/aju.jsp" />
    		<forward name="fail" path="/main.jsp" />
    	</action>
    	<action path="/trx/ajuUpdate" name="ajuForm" type="com.benclaus.koperasi.action.trx.AjuAction" scope="request" parameter="dispatch" validate="false">
			<forward name="continue" path="/app/trx/ajuEdit.jsp" />
			<forward name="success" path="/trx/aju.do?dispatch=returned" />
			<forward name="fail" path="/app/trx/ajuEdit.jsp" />
		</action>
		
		<action path="/trx/real" name="realSearch" type="com.benclaus.koperasi.action.trx.RealisasiAction" scope="request" validate="false" parameter="dispatch">
    		<forward name="continue" path="/app/trx/real.jsp" />
    		<forward name="fail" path="/main.jsp" />
    	</action>
    	<action path="/trx/realUpdate" name="realForm" type="com.benclaus.koperasi.action.trx.RealisasiAction" scope="request" parameter="dispatch" validate="false">
			<forward name="continue" path="/app/trx/realEdit.jsp" />
			<forward name="success" path="/trx/real.do?dispatch=returned" />
			<forward name="fail" path="/app/trx/realEdit.jsp" />
		</action>
		
	</action-mappings>
</struts-config>