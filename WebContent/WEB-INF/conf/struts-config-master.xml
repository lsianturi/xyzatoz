<?xml version="1.0" encoding="UTF-8"?>
<!-- 
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN"
                               "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
  -->
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" 
		"http://struts.apache.org/dtds/struts-config_1_2.dtd">

<struts-config>
	<form-beans>
		<form-bean name="perusahaanSearch" type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="dispatch" type="java.lang.String" initial="prepare" />
			<form-property name="nama" type="java.lang.String" />
			<form-property name="industri" type="java.lang.String" />
			<form-property name="area" type="java.lang.String" />
			<form-property name="pageIndex" type="java.lang.String" initial="1" />	
			<form-property name="totalPage" type="java.lang.String" />
			<form-property name="totalSize" type="java.lang.String" />		
		</form-bean>
		
		<form-bean name="perusahaanForm" type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="dispatch" type="java.lang.String" />
			<form-property name="id" type="java.lang.Integer" />
			<form-property name="nama" type="java.lang.String" />
			<form-property name="alamat" type="java.lang.String" />
			<form-property name="area" type="java.lang.Integer" />
			<form-property name="industri" type="java.lang.Integer" />
		</form-bean>
		
		
		<form-bean name="pegawaiSearch" type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="dispatch" type="java.lang.String" initial="prepare" />
			<form-property name="nama" type="java.lang.String" />
			<form-property name="statusPegawai" type="java.lang.String" />
			<form-property name="statusSipil" type="java.lang.String" />
			<form-property name="domisili" type="java.lang.String" />
			<form-property name="pageIndex" type="java.lang.String" initial="1" />	
			<form-property name="totalPage" type="java.lang.String" />
			<form-property name="totalSize" type="java.lang.String" />		
		</form-bean>
		
		<form-bean name="pegawaiForm" type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="dispatch" type="java.lang.String" />
			<form-property name="id" type="java.lang.Integer" />
			<form-property name="nama" type="java.lang.String" />
			<form-property name="alamat" type="java.lang.String" />
			<form-property name="domisili" type="java.lang.String" />
			<form-property name="telepon" type="java.lang.String" />
			<form-property name="stsPegawai" type="java.lang.Integer" />
			<form-property name="stsSipil" type="java.lang.Integer" />
			<form-property name="tanggalMasuk" type="java.lang.String" />
			<form-property name="keterangan" type="java.lang.String" />
		</form-bean>
		
		<form-bean name="nasabahSearch" type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="dispatch" type="java.lang.String" initial="prepare" />
			<form-property name="nama" type="java.lang.String" />
			<form-property name="telepon" type="java.lang.String" />
			<form-property name="perusahaan" type="java.lang.String" />
			<form-property name="bank" type="java.lang.String" />
			<form-property name="jnsAnggota" type="java.lang.String" />	
			<form-property name="stsAnggota" type="java.lang.String" />
			<form-property name="agentId" type="java.lang.String" />
			<form-property name="anAgent" type="java.lang.Boolean" />
			<form-property name="pageIndex" type="java.lang.String" initial="1" />	
			<form-property name="totalPage" type="java.lang.String" />
			<form-property name="totalSize" type="java.lang.String" />
		</form-bean>
		
		<form-bean name="nasabahForm" type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="dispatch" type="java.lang.String" />
			<form-property name="id" type="java.lang.Integer" />
			<form-property name="nama" type="java.lang.String" />
			<form-property name="jnsKelamin" type="java.lang.Integer" />
			<form-property name="stsSipil" type="java.lang.Integer" />
			<form-property name="alamat" type="java.lang.String" />
			<form-property name="domisili" type="java.lang.String" />
			<form-property name="telepon" type="java.lang.String" />
			<form-property name="agentId" type="java.lang.Integer" />
			<form-property name="perusahaan" type="java.lang.Integer" />
			<form-property name="bankId" type="java.lang.Integer" />
			<form-property name="bagian" type="java.lang.String" />
			<form-property name="noRekening" type="java.lang.String" />
			<form-property name="strTglMasuk" type="java.lang.String" />
			<form-property name="strTglPayroll" type="java.lang.String" />
			<form-property name="stsKrywn" type="java.lang.Integer" />
			<form-property name="jnsAnggota" type="java.lang.Integer" />
			<form-property name="stsAnggota" type="java.lang.Integer" />
			<form-property name="noRekeningRef" type="java.lang.String" />
			<form-property name="namaRef" type="java.lang.String" />
			<form-property name="keterangan" type="java.lang.String" />
			<form-property name="anAgent" type="java.lang.Boolean" />
		</form-bean>
		
	</form-beans>
	
	<action-mappings>
		<action path="/perusahaan" name="perusahaanSearch" type="com.benclaus.koperasi.action.master.PerusahaanAction" scope="request" validate="false" parameter="dispatch">
    		<forward name="continue" path="/app/master/perusahaan.jsp" />
    		<forward name="history" path="/app/master/perusahaanHistory.jsp"></forward>		
    		<forward name="fail" path="/main.jsp" />
    	</action>
    	<action path="/updatePerusahaan" name="perusahaanForm" type="com.benclaus.koperasi.action.master.PerusahaanAction" scope="request" parameter="dispatch" validate="false">
			<forward name="success" path="/perusahaan.do?dispatch=returned" />
			<forward name="continue" path="/app/master/perusahaanEdit.jsp" />
			<forward name="fail" path="/app/master/perusahaanEdit.jsp" />
		</action>
		
		<action path="/pegawai" name="pegawaiSearch" type="com.benclaus.koperasi.action.master.PegawaiAction" scope="request" validate="false" parameter="dispatch">
    		<forward name="continue" path="/app/master/pegawai.jsp" />	
    		<forward name="fail" path="/main.jsp" />
    	</action>
    	<action path="/updatePegawai" name="pegawaiForm" type="com.benclaus.koperasi.action.master.PegawaiAction" scope="request" parameter="dispatch" validate="false">
			<forward name="success" path="/pegawai.do?dispatch=returned" />
			<forward name="continue" path="/app/master/pegawaiEdit.jsp" />
			<forward name="fail" path="/app/master/pegawaiEdit.jsp" />
		</action>
		
		<action path="/nasabah" name="nasabahSearch" type="com.benclaus.koperasi.action.master.NasabahAction" scope="request" validate="false" parameter="dispatch">
    		<forward name="continue" path="/app/master/nasabah.jsp" />
    		<forward name="view" path="/app/master/nasabahView.jsp" />
    		<forward name="history" path="/app/master/nasabahHistory.jsp"></forward>	
    		<forward name="fail" path="/main.jsp" />
    	</action>
    	<action path="/updateNasabah" name="nasabahForm" type="com.benclaus.koperasi.action.master.NasabahAction" scope="request" parameter="dispatch" validate="false">
			<forward name="success" path="/nasabah.do?dispatch=returned" />
			<forward name="continue" path="/app/master/nasabahEdit.jsp" />
			<forward name="fail" path="/app/master/nasabahEdit.jsp" />
		</action>
		
	</action-mappings>
</struts-config>